<h1>Results</h1>
{{#each jobSearchList}}
<div class="job-preview">
  <div class="img-section"> <img src="{{this.image}}" alt="job icon"> </div>
  <div class="content-section">
    <h3>{{this.title}}</h3>
    <h4>{{this.city}}</h4>
    <a href="/jobs/{{this.id}}">View details</a>
    <button class="favorites" id="{{this.id}}">Add to favs</button>
  </div>
</div>
{{/each}}